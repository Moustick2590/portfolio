<section class="container-fluid" id="form_map">
    <div class="container">
        <div class="row justify-content-around">
            <div class="col-10 col-md-6" id="form_contact">
                <h2>Contactez moi par mail :</h2>
                <span id="msg_all"></span>
                <form action="process.php" id="contact" class="d-flex flex-column align-items-center" method="POST">
                    <label class="nom" for="nom">Votre Nom</label>
                    <input id="nom" name="nom" type="text">
                    <span id="msg_nom"></span>
                    <label for="email">Votre Email</label>
                    <input id="email" name="email" type="email">
                    <span id="msg_email"></span>
                    <label class="sujet" for="sujet">Le Sujet</label>
                    <input id="sujet" name="sujet" type="text">
                    <span id="msg_sujet"></span>
                    <label for="message">Votre Message</label>
                    <textarea id="message" name="message" rows="10"></textarea>
                    <span id="msg_message"></span>
                    <input type="submit" id="envoi_mail" value="Envoyer le mail" />
                </form>
            </div>
            <!-- end of #form_contact -->
            <div alt="carte géographique" class="col-10 col-md-6" id="map"></div>
        </div>
    </div>
</section>
