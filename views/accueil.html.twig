                    <ul>
                        <li class="nom_projet">
                            <h2>
                                {{data.nom}}</h2>
                        </li>
                        <li class="description_projet">
                            {{data.description}}
                        </li>
                        <li class="contrib_projet">
                            {{data.contributeurs}}
                        </li>
                        <li class="lien_page_projet">
                            <a href="{{ sub_path }}/{{data.nom}}">En savoir plus</a>
                        </li>
                    </ul>
                </figcaption>
            </figure>
